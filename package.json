{
  "name": "wota-spotter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "api": "tsx server/api.ts",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "prisma:generate": "prisma generate --schema=./prisma/wota.schema.prisma && prisma generate --schema=./prisma/cms.schema.prisma",
    "prisma:studio:wota": "prisma studio --schema=./prisma/wota.schema.prisma",
    "prisma:studio:cms": "prisma studio --schema=./prisma/cms.schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^6.19.0",
    "geodesy": "^2.4.0",
    "prisma": "^6.19.0",
    "vue": "^3.5.24"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/geodesy": "^2.2.8",
    "@types/node": "^24.10.1",
    "@vant/auto-import-resolver": "^1.3.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "tsx": "^4.20.6",
    "typescript": "~5.9.3",
    "unplugin-vue-components": "^30.0.0",
    "vant": "^4.9.21",
    "vite": "^7.2.4",
    "vitest": "^4.0.13",
    "vue-tsc": "^3.1.4"
  }
}
