// CMS Database Schema for User Authentication
// This schema connects to the CMS database

generator client {
  provider = "prisma-client-js"
  output   = "../node_modules/@prisma/cms-client"
}

datasource db {
  provider = "mysql"
  url      = env("CMS_DATABASE_URL")
}

